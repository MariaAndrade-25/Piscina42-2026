#include <unistd.h>

print_ reverse_alphabet(void)
{
    char	c;

    c = 'z';
    while (c > = 'a')
    write(1, &c, 1);
        c--;
    }
    //* 
    int main (void)
    {
        print_reverse_alphabet();
        return (0);
    }
///*/